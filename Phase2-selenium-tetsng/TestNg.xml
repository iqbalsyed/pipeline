<?xml version="1.0" encoding="UTF-8"?>
<suite name="eComWebapp TestSuite">
	<listeners>
		<listner class-name = "org.uncommons.reportng.HTMLReporter"></listner>
		<listner class-name = "org.uncommons.reportng.JUnitXMLReporter"></listner>
	</listeners>

	<test name="Amazon Test">
		<classes>
			<class name="com.simplilearn.phase2.Phase2_selenium_tetsng.amazonHomepageTest"></class>
			<class name="com.simplilearn.phase2.Phase2_selenium_tetsng.AmazonLinkVerificationByCss"></class>
			<class name="com.simplilearn.phase2.Phase2_selenium_tetsng.AmazonLinVerificationByXpath"></class>
			<class name="com.simplilearn.phase2.Phase2_selenium_tetsng.AmazonSearchTest"></class>
		</classes>
	</test>
	<test name="Group Test">
		<classes>
			<class name="com.simplilearn.phase2.Phase2_selenium_tetsng.AttributeTest"></class>
		</classes>
	</test>
	<test name="Other Test">
		<classes>
			<class name="com.simplilearn.phase2.Phase2_selenium_tetsng.TestNgStandardAnnotaion"></class>
			<class name="com.simplilearn.phase2.Phase2_selenium_tetsng.AssertionTest"></class>
		</classes>
	</test>
</suite>